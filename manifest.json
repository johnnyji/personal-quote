{
  "manifest_version": 2,
  "name": "PersonalQuote",
  "description": "A personal quote tab",
  "version": "1.0.0",
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "background": {
    "scripts": [
      "./build/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "identity",
    "storage",
    "tabs",
    "https://graph.facebook.com/*",
    "https://www.facebook.com/*",
    "https://api.medium.com/*",
    "https://medium.com/*",
    "https://www.reddit.com/*"
  ]
}
